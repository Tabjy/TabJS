{
  "port": 8080,
  "host": "127.0.0.1",
  "indexFile": ["index.html", "index.htm","welcome.html"],
  "indexController": ["index.js", "main.js"],
  "uploadDir": "/uploads",
  "compression": {
    "enabled": true,
    "priority": ["gzip", "deflate"],
    "fileType": ["css", "js", "html", "json", "txt", "svg"]
  },
  "customErrorController": {
    "enabled": false,
    "controllerPath": "error.js"
  },
  "listDirectory": {
    "enabled": true,
    "display": {
      "size": true,
      "accessTime": false,
      "modifiedTime": true,
      "changeTime": false,
      "birthTime": false,
      "mimetype": true,
      "description": true
    }
  },
  "timeout": "0.5 * 60",
  "router": [
  ]
}
